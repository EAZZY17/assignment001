{
  "testName": "Sign In and Edit Project Test",
  "description": "Test to verify user can sign in and edit an existing project",
  "prerequisites": [
    "User account must exist",
    "At least one project must exist",
    "User must be signed out"
  ],
  "steps": [
    {
      "action": "navigate",
      "url": "/signin",
      "description": "Navigate to sign in page"
    },
    {
      "action": "fill",
      "field": "email",
      "value": "testuser@example.com",
      "description": "Enter email address"
    },
    {
      "action": "fill",
      "field": "password",
      "value": "testpassword123",
      "description": "Enter password"
    },
    {
      "action": "submit",
      "form": "signin",
      "description": "Submit sign in form"
    },
    {
      "action": "verify",
      "expected": "Redirect to projects page",
      "url": "/projects",
      "description": "Verify successful sign in"
    },
    {
      "action": "click",
      "element": "Edit button on existing project",
      "description": "Click Edit button for a project"
    },
    {
      "action": "fill",
      "field": "title",
      "value": "Updated Project Title",
      "description": "Update project title"
    },
    {
      "action": "fill",
      "field": "description",
      "value": "Updated description",
      "description": "Update project description"
    },
    {
      "action": "submit",
      "form": "project",
      "description": "Submit updated project form"
    },
    {
      "action": "verify",
      "expected": "Project shows updated information",
      "element": "Updated Project Title",
      "description": "Verify project was updated successfully"
    }
  ],
  "expectedResult": "User signs in successfully and edits an existing project with updated information displayed"
}

